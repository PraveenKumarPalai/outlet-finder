extends layout

block content
  .main.container
    .row
      .col-md-6.col-md-offset-3
        h1.display-4.m-b-2 Search Outlet
        form(method='GET' action='/get_outlet')
          div.form-group
            label(for='address') Address
            input.form-control(type='text' id='address' placeholder='Address' name='address')
          button.btn.btn-primary(type='submit') Find 